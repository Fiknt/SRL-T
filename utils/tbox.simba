(*
TBox
=========
TBox related methods
*)

function TBox.ToRectangle(): TRectangle; 
begin
  Result.Top   := [Self.X1, Self.Y1];
  Result.Right := [Self.X2, Self.Y1];
  Result.Btm   := [Self.X2, Self.Y2];
  Result.Left  := [Self.X1, Self.Y2];
end;
